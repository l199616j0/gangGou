/* pages/project/project.wxss */
.search_bar
{
  width:100%;
  float: left;
  padding: 3px 0px;
}
.city
{
  width:20%;
  float: left;
  text-align: center;
  font-size: 12px;
  line-height: 24px;
  height: 24px;
}
.search
{
  width:60%;
  float: left;
  border: 1px solid #d0d0d0;
  border-radius: 22rpx;
  padding-left: 30px;
  font-size: 12px;
}
.search input
{
  width:90%;
}
.searchcion
{
  float: left;
  margin-top:6px;
  margin-right: 5px;
}
.body{
  background: rgb(243, 242, 242);
}
.choice{
  width: 100%;
  padding-top: 35px;
  padding-bottom: 5px;
  position: absolute;
  border-bottom: 1px solid #DADADA;
}
.choice>view{
  display: inline-block;
  margin: 2px;
}
.section{
  width: 20%;
}
.sectionCity{
  width: 25%;
}
.choice text{
  color: #dfdfdf;
}
.picker{
  text-align: center;
}

.projectlist{
  width: 100%;
  position: absolute;
  margin-top: 60px;
}
.project{
  margin-top: 10px;
  border-bottom: 1px solid #DADADA;
  width: 100%;
  /* margin-left: 15px; */
}
.project view{
 /* margin-left: 5px; */
  display: inline-block;
}

.projectText{
  margin-left: 15px;
  line-height: 16px;
  text-align: left;
  width: 90%;
  text-overflow:ellipsis;/*设置标签超出长度时用省略号代替*/
  white-space: nowrap;/*white-space是设置文本不换行*/
  overflow: hidden;/*overflow设置标签超出部分自动隐藏*/
  color:#050505;
  margin-bottom: 5px;
}
.label{
  text-align: left;
  /* width: 20%; */
  /* color: #8d8c8c; */
  background: #fffcfc;
  border: 1px solid #ff9378;
  border-radius: 10rpx;
  margin: 2px;
  padding:1px 2px;
  color: #fd2b2b;
}
.projectleibie{
  margin-left: 15px;
}
.projectTime{
  color: #c5c5c5;
  padding-bottom: 10px;
}