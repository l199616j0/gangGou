.body{
  background: rgb(243, 242, 242);
  height: 423px;
}
.main{
  width: 100%;
  /* margin: 0 4%; */
  background: white;
}
.main>view{
  margin-top: 10px;
  width: 33.3%;
  height: 60px;
  text-align: center;
  display: inline-block;
}
.main image{
  width:100%;
  height:50%;
}

.new, .project{
  width: 100%;
  height: 100%;
  margin: 0 ;
  margin-top: 10px;
  background: white;
}
.new_title, .project_title{
  height: 30px;
  padding-top: 5px;
  border-bottom: 1px solid #DADADA;
  margin-bottom: 2px;
  font-size: 15px;
}
.new_title text{
  margin-left: 30px;
}
.Modular{
  margin-top: 10px;
  width: 100%;
  height: 100px;
  text-align: center;
}
.myself{
  border-left: 1px solid	#F0F0F0;
  border-right: 1px solid	#F0F0F0;
  border-bottom: 1px solid	#F0F0F0;
  width: 100%;
  height: 30px;
}
.myself text{
  margin-left: 30px;
  line-height: 25px;
  font-size: 13px;

}
